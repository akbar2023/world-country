pipeline {
    agent any

    stages {
        stage('Munit Test') {
            steps {
                echo 'Testing..'
                mvn 'mvn clean test'
            }
        }r
        stage('Build') {
            steps {
                echo 'Building..'
                bat 'mvn clean install'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying to cloudHub...'
                bat '-DskipTests -Dmule.version=4.4.0 -Danypoint.username=akbar_khan1 -Danypoint.password=Mymulesoft@20 -Denv=Sandbox -Dappname=world-countries -Dbusiness=cap -DvCore=Micro -Dworkers=1'
                echo 'Deployed...'
            }
        }
    }
}